<%= render "side" %><h2>编辑</h2>
<div>
<%= form_tag do %>
    current_version_id<%= text_field_tag :current_version_id, params[:current_version_id],:value=>@version.current_version_id,:size=>8 %><br>
    current_comments<%= text_field_tag :current_comments, params[:current_comments],:value=>@version.current_comments,:size=>50 %><br>
    Rails:<br>
    comments<%= text_field_tag :rails_comments, params[:rails_comments],:value=>@version.rails_comments,:size=>8 %>
    branch_id<%= text_field_tag :rails_branch_id, params[:rails_branch_id],:value=>@version.rails_branch_id,:size=>8 %>
    commit_id<%= text_field_tag :rails_commit_id, params[:rails_commit_id],:value=>@version.rails_commit_id,:size=>8 %><br>
    MYSQL:<br>
    comments<%= text_field_tag :mysql_comments, params[:mysql_comments],:value=>@version.mysql_comments,:size=>8 %>
    branch_id<%= text_field_tag :mysql_branch_id, params[:mysql_branch_id],:value=>@version.mysql_branch_id,:size=>8 %>
    commit_id<%= text_field_tag :mysql_commit_id, params[:mysql_commit_id],:value=>@version.mysql_commit_id,:size=>8 %><br>
    CTP:<br>
    comments<%= text_field_tag :ctp_comments, params[:ctp_comments],:value=>@version.ctp_comments,:size=>8 %>
    branch_id<%= text_field_tag :ctp_branch_id, params[:ctp_branch_id],:value=>@version.ctp_branch_id,:size=>8 %>
    commit_id<%= text_field_tag :ctp_commit_id, params[:ctp_commit_id],:value=>@version.ctp_commit_id,:size=>8 %><br>
    <%= submit_tag "提交"%><% end %>
<%= link_to "返回", :controller=>"admin",:action=>"currentversion"%>
</div>
<br><br><br><br>