<script>
    function approve(oid){
        $.get("/admin/organapprove/"+oid,function(data){
           $(".approve"+oid).html("通过");
        });
    }

    function enroll(aid){
        $.get("/personmanagement/enrollapprove",{aid:aid},function(data){
            $("#enroll"+aid).html("");
        });
    }
    function organ(oid,uid){
        $.get("/usersurvey/dreamin",{username:uid},function(data){
            $("#organ"+oid).html("");
        });
    }

    $(document).ready(function(){
        $('#products,#funds,#interests_r,#interests_c,#productsupdate,#bankfinance,#reserve,#bankproducts,#organ,#category1,#category2,#financial,#company,#poundage').dataTable();
        var oTable = $('#bankfinance').dataTable();
        $("#keywords").click( function() { oTable.fnDraw(); } );

        /* Initialise datatables */
        var oTable = $('#blog').dataTable({
            "aaSorting": [[ 5, "desc" ]]
        });
        var oTable = $('#users').dataTable({
            "aaSorting": [[ 0, "desc" ]]
        });
        var oTable = $('#organuser').dataTable({
            "aaSorting": [[ 0, "desc" ]]
        });
        var oTable = $('#activity').dataTable({
            "aaSorting": [[ 0, "desc" ]]
        });
        var oTable = $('#enroll').dataTable({
            "aaSorting": [[ 0, "desc" ]]
        });
    });
    function deleteajax(id,type){
        $.get("/admin/"+type+"/"+id,function(data){
            if(data=="s"){
                alert("删除成功！");
                location.reload();
            }
            else{
                alert("删除失败！");
                location.reload();
            }
        });
    }

    function interestconfirm(username,date,value,ordernum,pname){
        $.get("/admin/interestconfirm",{username:username,date:date,recordvalue:value,ordernum:ordernum,pname:pname},function(data){
            if(data=="s"){
                alert("确认成功！");
                location.reload();
            }
            else if(data=="f"){
                alert("确认失败！");
                location.reload();
            }

        })
    }
</script>