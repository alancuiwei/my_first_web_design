                }
                else{
                    total=parseInt((tot1+jixu)/100)*100;//总资产
                    xian=parseInt((parseFloat(tot3)*3)/100)*10;
                }
                feng=parseInt((total*(80-parseFloat($("#age").attr("value").replace(/,/g, "")))/100)/100)*100;
