        var chuxu=parseInt(sell_house_account)+parseInt(family_saving_account)+parseInt(borrowing_account)-parseInt($("#display-ctotal").html().replace("￥",""))
    <% else %>
        var chuxu=parseInt(family_saving_account)+parseInt(borrowing_account)-parseInt($("#display-ctotal").html().replace("￥",""))
    <% end %>
        if(chuxu<0){chuxu=0;}
        var month=0
    <% if @userdatamonth!=nil && @userdatamonth.must_expense_month!=nil && @userdatamonth.must_expense_month!=0 %>
       month=parseInt(chuxu/parseInt(<%= @userdatamonth.must_expense_month %>));
    <% end %>
        if(month==0){
            housecolor1=2;
            $("#closing-graphic").css('color', 'rgb(189, 55, 38)');
            $("#close-breakdown tr:last td:last").css('color', 'rgb(189, 55, 38)');}
        else if(month<3){
            housecolor1=1;
            $("#closing-graphic").css('color', 'rgb(217, 179, 11)');
            $("#close-breakdown tr:last td:last").css('color', 'rgb(217, 179, 11)');}
        else{
            housecolor1=0;
            $("#closing-graphic").css('color', 'rgb(103, 169, 83)');
            $("#close-breakdown tr:last td:last").css('color', 'rgb(103, 169, 83)');
        }
        $("#chuxu").html(chuxu);
        $("#month,#month2").html(month);
        debtguage.refresh(parseInt(parseInt($("#debthouse").html())*100*12/parseInt($("#salary_account").attr("value"))));
        if(parseInt(parseInt($("#debthouse").html())*100*12/parseInt($("#salary_account").attr("value")))<36){
            var housecolor2=0
            $("#xiaofei").css('color', 'rgb(103, 169, 83)');
        }
        else if(parseInt(parseInt($("#debthouse").html())*100*12/parseInt($("#salary_account").attr("value")))<50){
            var housecolor2=1
            $("#xiaofei").css('color', 'rgb(217, 179, 11)');
        }
        else{
            var housecolor2=2
            $("#xiaofei").css('color', 'rgb(189, 55, 38)');
        }
        var housecolor=Math.max(housecolor1,housecolor2)
        $("#hm-icon").removeClass('hsprite-green');
        $("#hm-icon").removeClass('hsprite-yellow');
        $("#hm-icon").removeClass('hsprite-red');
        $("#smarttip1,#smarttip2,#smarttip3").addClass('hide');
        if(housecolor==2){
            $("#hm-icon").addClass('hsprite-red');
            $(".housecolor").css('color', 'rgb(189, 55, 38)');
            $("#smarttip3").removeClass('hide');
        }
        else if(housecolor==1){
            $("#hm-icon").addClass('hsprite-yellow');
            $(".housecolor").css('color', 'rgb(217, 179, 11)');
            $("#smarttip2").removeClass('hide');
        }
        else{
            $("#hm-icon").addClass('hsprite-green');
            $(".housecolor").css('color', 'rgb(103, 169, 83)');
            $("#smarttip1").removeClass('hide');
        }
        $("#shoufu3").html($("#display-ctotal").html());
        $("#benjin").html(parseInt($("#daikuan_total").html().replace("￥",""))+parseInt($("#daikuan_total_sy").html().replace("￥","")));
        $("#benjin2").html("￥"+(parseInt($("#daikuan_total").html().replace("￥",""))+parseInt($("#daikuan_total_sy").html().replace("￥",""))).toString());
        $("#daikuan").html(parseInt($("#accrual").html().replace("￥",""))+parseInt($("#accrual_sy").html().replace("￥","")));
        $("#daikuan2").html("￥"+(parseInt($("#accrual").html().replace("￥",""))+parseInt($("#accrual_sy").html().replace("￥",""))).toString());
        $("#shoufu2").html(parseInt($("#display-shoufu").html().replace("￥","")));
        $("#qita").html(parseInt($("#display-ctotal").html().replace("￥",""))-parseInt($("#display-shoufu").html().replace("￥","")));
    }

    $("#payment-decrease,#savings-increase").click(function(){
        if(parseInt($("#downpayment").attr("value"))>=10000){
            $("#display-dp,#display-shoufu").html("￥"+(parseInt($("#downpayment").attr("value"))-10000).toString())
            $("#downpayment").attr("value", parseInt($("#downpayment").attr("value"))-10000);
            $("#downpaymentrate").attr("value",parseInt(parseInt($("#downpayment").attr("value"))/parseInt(parseInt($("#display-hv").html().replace("￥","")))*100));

            
            $("#surplussavings").attr("value", parseInt($("#surplussavings").attr("value"))+10000);
            update();
        }
    });

    $("#payment-increase,#savings-decrease").click(function(){
        if(parseInt($("#surplussavings").attr("value"))>=10000){
            $("#display-dp,#display-shoufu").html("￥"+(parseInt($("#downpayment").attr("value"))+10000).toString())
            $("#downpayment").attr("value", parseInt($("#downpayment").attr("value"))+10000);
            $("#downpaymentrate").attr("value",parseInt(parseInt($("#downpayment").attr("value"))/parseInt(parseInt($("#display-hv").html().replace("￥","")))*100));

            $("#surplussavings").attr("value", parseInt($("#surplussavings").attr("value"))-10000);
            update();
        }
    });

    $("#rate-decrease").click(function(){
        if(parseInt($("#downpaymentrate").attr("value"))>=10){
            $("#downpaymentrate").attr("value", parseInt($("#downpaymentrate").attr("value"))-5);
        }
        else{
            $("#downpaymentrate").attr("value",5);
        }
        update();
    });

    $("#rate-increase").click(function(){
        if(parseInt($("#downpaymentrate").attr("value"))<=95){
            $("#downpaymentrate").attr("value", parseInt($("#downpaymentrate").attr("value"))+5);
        }
        else{
            $("#downpaymentrate").attr("value",100);
        }
        update();
    });

    function houseweiyi(i){
        if(i==0){
            if($("#buy_house_attribute").attr("value")=='1'){
                $("#downpaymentrate").attr("value",70);
            }
            else{
                $("#downpaymentrate").attr("value",30);
            }
        }
        else{
            if($("#is_first_house").attr("value")=='0'){
                $("#downpaymentrate").attr("value",70);
            }
            else{
                $("#downpaymentrate").attr("value",30);
            }
        }
        update();
    }

