    function housetype(){
        if($("#buy_house_type").attr("value")=='1'){
            $("#inputs-credit").addClass("hide");
            $(".inputs-location").removeClass("hide");
        }
        else{
            $("#inputs-credit").removeClass("hide");
            $(".inputs-location").addClass("hide");
        }
        if(($("#buy_house_type").attr("value")=='0' && $("#buy_house_attribute").attr("value")=='1') || ($("#buy_house_type").attr("value")=='1' && $("#is_first_house").attr("value")=='0')){
            $("#downpaymentrate").attr("value",70);
        }
        else{
            $("#downpaymentrate").attr("value",30);
        }
        update();
    }

