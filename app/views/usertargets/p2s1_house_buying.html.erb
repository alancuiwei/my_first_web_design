        var all_total1 = month_money1 * month;//还款总额
        var accrual = Math.round( (all_total1 - daikuan_total) *100)/100; //支付利息款
        $("#daikuan_total_sy").html("￥"+daikuan_total.toString())
        $("#accrual_sy").html("￥"+parseInt(accrual).toString())
        $("#all_total_sy").html("￥"+parseInt(daikuan_total+accrual).toString())
        $("#month_money_sy").html("￥"+parseInt(month_money1).toString())
        $("#debthouse").html(parseInt(month_money1).toString())
        $("#debthouse2").html("￥"+parseInt(month_money1).toString())
        $("#debtall").html(parseInt(month_money1).toString())
    }

