    function show_all(){
        $.get("personaltrading", { isorder_show: "isorder_show" },function(data,status){
            isorder_search=2;
            hash_isorder_show.clear();
            for(var i=0;i<data.length;i++){
                hash_isorder_show.add(data[i].pairname,1)
            }
        } );
    }

    function show_none(){
        $.get("personaltrading", { isorder_show: "isorder_show" },function(data,status){
            isorder_search=0;
            hash_isorder_show.clear();
            for(var i=0;i<data.length;i++){
                if(data[i].isorder==null)
                hash_isorder_show.add(data[i].pairname,1)
                else
                    hash_isorder_show.add(data[i].pairname,0)
            }
        } );
    }

    function show_done(){
        $.get("personaltrading", { isorder_show: "isorder_show" },function(data,status){
            isorder_search=1;
            hash_isorder_show.clear();
            for(var i=0;i<data.length;i++){
